{
  "name" : "visualizador-algoritmos",
  "version-string" : "1.0.0",
  "dependencies" : [
    {
      "name" : "glfw3",
      "default-features" : true
    },
    {
      "name" : "glad",
      "default-features" : true
    },
    {
      "name" : "glm",
      "default-features" : true
    },
    {
      "name" : "opengl",
      "default-features" : true
    },
    {
      "name" : "implot",
      "default-features" : true
    },
    {
      "name" : "imgui",
      "features" : [
        "glfw-binding",
        "opengl3-binding",
        "dx11-binding",
        "win32-binding",
        "docking-experimental"
      ]
    },
    {
      "name" : "glew",
      "default-features": true
    },
    {
      "name" : "range-v3",
      "default-features": true
    },
    {
      "name" : "tbb",
      "default-features": true
    }
  ]
}